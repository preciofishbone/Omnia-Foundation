{
  "less": {
    "compile": {
      "tenantResource": {
        "files": [
          "TenantResources/**/*.less"
        ],
        "dist": "TenantResources"
      }
    },
    "watch": [ "**/*.less" ],
    "includeLessPath": [
      "TenantResources"
    ]
  },
  "webpack": {
    "basePaths": [ "TenantResources" ],
    "compileFiles": [
      "TenantResources/**/*.js"
    ],
	"flatModules":[],
    "ignoreFiles": [
    ],
    "watch": [ "TenantResources//**/*.js" ]
  },
  "aot": {
    "typesForCompileAotTsFiles": [
      "node_modules/@omnia/foundation/tools/TaskRunner/typings/omf.aot.d.ts",
      "node_modules/@omnia/foundation/tools/TaskRunner/typings/core-js/index.d.ts"
    ],
    "compileAotTsFiles": [
      "TenantResources/**/*.ngfactory.ts"
    ],
    "webpackCompileFiles": [
    ],
    "ignoreFiles": [],
    "cleanBefore": [
      "TenantResources/**/*.ngfactory.ts"
    ],
    "cleanAfter": [ "TenantResources/**/*.ngsummary.json" ],
    "copyOutputAotFilesToTenantResources": [],
    "ngc": {
      "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "lib": [ "es2015", "dom" ],
        "declaration": false,
        "sourceMap": false,
        "removeComments": false,
        "noImplicitAny": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "types": []
      },
      "files": [
        "node_modules/@omnia/foundation/tools/TaskRunner/typings/omf.aot.d.ts",
        "TaskRunner/omf.aot.modules.ts"
      ],
      "angularCompilerOptions": {
        "genDir": ".",
        "skipMetadataEmit": true,
        "skipTemplateCodegen": false,
        "debug": false
      },
      "exclude": [
        "node_modules",
        "TaskRunner"
      ]
    }
  }
}